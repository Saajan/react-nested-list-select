{
  "name": "react-object-list-select",
  "version": "1.0.2",
  "license": "MIT",
  "description": "List with selectable and navigable items",
  "author": "Saajan",
  "repository": "Saajan/react-object-list-select",
  "contributors": [
    "Hawken Rives <hawkrives@gmail.com>",
    "Saajan <saajan.sn@gmail.com>"
  ],
  "keywords": [
    "react-component"
  ],
  "main": "dist/index.js",
  "files": [
    "dist/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "deploy": "gh-pages-deploy",
    "lint": "eslint --cache *.js example/*.js",
    "prepublishOnly": "npm run build",
    "prettify": "prettier --write src/*.js example/*.{css,js}",
    "post-deploy": "npm run clean",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "gh-pages-deploy": {
    "staticpath": "example",
    "prep": [
      "example"
    ],
    "noprompt": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": true,
    "bracketSpacing": false,
    "trailingComma": "all"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "babel-loader": "^8.0.6",
    "babelify": "^10.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-react": "^7.16.0",
    "gh-pages-deploy": "^0.5.1",
    "prettier": "^1.18.2",
    "react": ">=16"
  }
}
